{
  "servicePrefixes": [
    "api"
  ],
  "routes": [
    {
      "key": "default",
      "value": "index.html"
    }
  ],
  "resources": [
    {
      "type": "html",
      "content": "text/html"
    },
    {
      "type": "js",
      "content": "application/x-javascript"
    },
    {
      "type": "css",
      "content" : "text/css"
    },
    {
      "type": "woff",
      "content": "application/font-woff"
    },
    {
      "type": "jpg",
      "content": "image/jpeg"
    },
    {
      "type": "ico",
      "content": "image/x-icon"
    },
    {
      "type": "gif",
      "content": "image/gif"
    }
  ],
  "rewrites": [
    {
      "pattern": "^(article|page|editor|markdown){1}($|/)",
      "value": "",
      "mode": "Override"
    }
  ]
}